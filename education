          <script>
            //쿠키 가져오기
            function getCookie(cookie_name) {
              var x, y;
              var val = document.cookie.split(';');

              for (var i = 0; i < val.length; i++) {
                x = val[i].substr(0, val[i].indexOf('='));
                y = val[i].substr(val[i].indexOf('=') + 1);
                x = x.replace(/^\s+|\s+$/g, ''); // 앞과 뒤의 공백 제거하기

                if (x == cookie_name) {
                  var _pk_id = unescape(y).split('.');
                  return _pk_id[0]; // unescape로 디코딩 후 값 리턴
                }
              }
            }
            var MINFO_ARR = { all: [], my: [] };
            var uid = getCookie('_pk_id.sen') || "0443a992-fc14-3c28-e413-660570075283";
            if (uid != null) {
              $.ajax({
                type: "GET",
                contentType: 'application/x-www-form-urlencoded; charset=UTF-8',
                dataType: "JSON",
                url: "https://www.sen.go.kr/blueworks/openapi/top/menus?searchCount=10&uid=" + uid,
                success: function (response) {
                  if (response.returnCode == 0) {
                    var myInfo = $("#myInfo");
                    var allInfo = $("#allInfo");
                    var data;
                    // 전체 관심정보
                    //console.log("** response.total.length :  "+response.total.length);
                    if (response.total.length !== 0) {
                      $.each(response.total, function (index, item) {
                        //console.log("전체", index, item)
                        data = "<li><a href='https://" + item.menuUrl + "'>" + item.menuName + "</a></li>";
                        //allInfo.append(data);
                        MINFO_ARR.all.push(data);
                      });
                    }

                    //나의 관심정보
                    //console.log("** response.user.length :  "+response.user.length);
                    if (response.user.length !== 0) {
                      $.each(response.user, function (index, item) {
                        //console.log("나의", index, item)
                        data = "<li><a href='https://" + item.menuUrl + "'>" + item.menuName + "</a></li>";
                        //myInfo.append(data);
                        MINFO_ARR.my.push(data);
                      });
                    }
                  }
                },
                error: function () {
                  console.log("error")
                }
              });
            }
          </script>
        
